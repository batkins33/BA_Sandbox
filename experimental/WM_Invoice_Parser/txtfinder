with pdfplumber.open(pdf_path) as pdf:
    text = ""
    for i, page in enumerate(pdf.pages):
        page_text = page.extract_text()
        if i < 2:  # Print the first two pages for inspection
            print(f"--- PAGE {i+1} ---")
            print(page_text[:2000])
        text += page_text + "\n"
